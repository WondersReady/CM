                <Anchor side="Right" relative="$parent/Item0" pos="Max" offset="0"/>
            </Frame>

            <Frame type="Frame" name="Item2" template="CM_CommanderSelectionDetailsContainerItems/CommanderAbilTileListItemTemplate">
