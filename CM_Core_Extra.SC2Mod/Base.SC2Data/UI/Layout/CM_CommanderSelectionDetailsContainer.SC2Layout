                <Alpha val="31"/>
            </Frame>

            <Frame type="ProgressBar" name="LevelProgressBar">
                <Anchor side="Top" relative="$parent/LevelLabel" pos="Mid" offset="0"/>
                <Anchor side="Bottom" relative="$parent/LevelLabel" pos="Mid" offset="0"/>
                <Anchor side="Left" relative="$parent/LevelLabel" pos="Mid" offset="-1"/>
                <Anchor side="Right" relative="$parent/LevelLabel" pos="Mid" offset="-1"/>
                <Width val="156"/>
                <Height val="156"/>
                <Color val="FFFFFF"/>
                <ColorStep val="FFFFFF"/>
                <UseFillContainer val="False"/>
                <Circular val="True"/>
                <CircularStart val="0"/>
                <CircularEndOffset val="360"/>
                <MaxValue val="15"/>
                <Value val="1"/>
                
                <Frame type="Image" name="BorderImage">
                    <Anchor side="Top" relative="$parent" pos="Mid" offset="0"/>
                    <Anchor side="Bottom" relative="$parent" pos="Mid" offset="0"/>
                    <Anchor side="Left" relative="$parent" pos="Mid" offset="0"/>
                    <Anchor side="Right" relative="$parent" pos="Mid" offset="0"/>
                    <Texture val="@UI/SC2_UI_Coop_CommanderIcon_CircularProgress_Border_Normal"/>
                </Frame>
                
                <Frame type="Frame" name="FillImageContainer">
                    <Anchor relative="$parent"/>
                    
                    <Frame type="Image" name="FillImage">
                        <Anchor relative="$parent"/>
                        <Texture val="@UI/SC2_UI_Coop_CommanderIcon_CircularProgress_Fill_Normal"/>
                        <TextureType val="Circular"/>
                    </Frame>
                </Frame>
                
                <Frame type="Image" name="MaxLevelFillImage">
                    <Anchor relative="$parent"/>
                    <Visible val="False"/>
                    <Texture val="@UI/SC2_UI_Coop_CommanderIcon_CircularProgress_Fill_Max"/>
                </Frame>
            </Frame>
            
            <Frame type="Image" name="RandomIcon">
                <Anchor side="Top" relative="$parent/LevelLabel" pos="Mid" offset="0"/>
                <Anchor side="Bottom" relative="$parent/LevelLabel" pos="Mid" offset="0"/>
                <Anchor side="Left" relative="$parent/LevelLabel" pos="Mid" offset="-1"/>
                <Anchor side="Right" relative="$parent/LevelLabel" pos="Mid" offset="-1"/>
                <Texture val="@UI/SC2_UI_Coop_CommanderIcon_Bubble_Random"/>
                <Visible val="False"/>
            </Frame>
            
            <Frame type="Image" name="MaxLevelIcon">
                <Anchor side="Top" relative="$parent/LevelLabel" pos="Mid" offset="-1"/>
                <Anchor side="Bottom" relative="$parent/LevelLabel" pos="Mid" offset="-1"/>
                <Anchor side="Left" relative="$parent/LevelLabel" pos="Mid" offset="-2"/>
                <Anchor side="Right" relative="$parent/LevelLabel" pos="Mid" offset="-2"/>
                <Width val="143"/>
                <Height val="141"/>
                <ImageMaskTexture val="@UI/SC2_UI_Coop_CommanderIcon_Bubble_Mask"/>
                <Texture val="@UI/SC2_UI_Coop_CommanderIcon_MaxLevel_Large"/>
                <Visible val="false"/>
            </Frame>
            
            <Frame type="Label" name="LevelLabel">
                <Anchor side="Top" relative="$parent/CommanderDetailsBackground" pos="Mid" offset="0"/>
                <Anchor side="Bottom" relative="$parent/CommanderDetailsBackground" pos="Mid" offset="0"/>
                <Anchor side="Left" relative="$parent/CommanderDetailsBackground" pos="Mid" offset="1"/>
                <Anchor side="Right" relative="$parent/CommanderDetailsBackground" pos="Mid" offset="1"/>
                <Width val="150"/>
                <Style val="@Coop_CommanderLevel"/>
                <Text val="{$CoopStandardGlue_CommanderSelectionPanel/@Level}"/>
                <Options val="ScaleToPreventTruncation"/>
            </Frame>
            
            <Frame type="Label" name="CappedLevelLabel">
                <Anchor side="Top" relative="$parent/LevelLabel" pos="Min" offset="10"/>
                <Anchor side="Left" relative="$parent/CommanderDetailsBackground" pos="Mid" offset="1"/>
                <Anchor side="Right" relative="$parent/CommanderDetailsBackground" pos="Mid" offset="1"/>
                <Width val="150"/>
                <Style val="@Coop_CommanderLevel"/>
                <Text val="{$CoopStandardGlue_CommanderSelectionPanel/@Level}"/>
                <Visible val="True"/>
            </Frame>
            
            <Frame type="Label" name="CappedWarningLabel">
                <Anchor side="Bottom" relative="$parent/CappedLevelLabel" pos="Min" offset="13"/>
                <Anchor side="Left" relative="$parent/CommanderDetailsBackground" pos="Mid" offset="1"/>
                <Anchor side="Right" relative="$parent/CommanderDetailsBackground" pos="Mid" offset="1"/>
                <Style val="Coop_CommanderLevel_Warning"/>
                <Text val="@UI/ScreenCoopCampaign/XPCapped"/>
                <Visible val="{$parent/CappedLevelLabel/@Visible}"/>
            </Frame>
            
            <Frame type="Image" name="CappedIconUnderlay">
                <Anchor side="Top" relative="$parent/CappedIcon" pos="Mid" offset="0"/>
                <Anchor side="Bottom" relative="$parent/CappedIcon" pos="Mid" offset="0"/>
                <Anchor side="Left" relative="$parent/CappedIcon" pos="Mid" offset="0"/>
                <Anchor side="Right" relative="$parent/CappedIcon" pos="Mid" offset="0"/>
                <Texture val="@UI/SC2_UI_Coop_CappedIcon_Small_Underlay"/>
                <Visible val="{$parent/CappedLevelLabel/@Visible}"/>
            </Frame>
            
            <Frame type="Image" name="CappedIcon">
                <Anchor side="Top" relative="$parent/LevelProgressBar" pos="Min" offset="-8"/>
                <Anchor side="Left" relative="$parent/LevelProgressBar" pos="Mid" offset="0"/>
                <Anchor side="Right" relative="$parent/LevelProgressBar" pos="Mid" offset="0"/>
                <Texture val="@UI/SC2_UI_Coop_CappedIcon_Small_Overlay"/>
                <Visible val="{$parent/CappedLevelLabel/@Visible}"/>
            </Frame>
                        
            <StateGroup name="MaxLevelStateGroup">
                <DefaultState val="PrestigeMaxLevelState"/>
                
                <State name="Random">
                    <When type="Property" frame="$ancestor[@type=CommanderSelectionPanel]" Commander="Random"/>
                    <Action type="SetProperty" frame="LevelProgressBar" Visible="False"/>
                    <Action type="SetProperty" frame="LevelLabel" Visible="false"/>
                    <Action type="SetProperty" frame="CappedLevelLabel" Visible="false"/>
                    <Action type="SetProperty" frame="RandomIcon" Visible="True"/>
                </State>
                
                <State name="PrestigeMaxLevelState">
                    <When type="AnimationState" frame="$CoopStandardGlue_CommanderSelectionPanel" CommanderMaxLevel="MaxLevel"/> 
                    <When type="Property" frame="$Coop_MasteryLevelFrame" operator="GreaterThanOrEqual" MasteryLevel="1000"/>
                    <Action type="SetProperty" frame="LevelLabel" Visible="false"/>
                    <Action type="SetProperty" frame="LevelProgressBar/BorderImage" texture="@UI/SC2_UI_Coop_CommanderIcon_CircularProgress_Border_Max"/>
                    <Action type="SetProperty" frame="LevelProgressBar/FillImageContainer/FillImage" visible="false"/>
                    <Action type="SetProperty" frame="LevelProgressBar/MaxLevelFillImage" visible="True"/>
                    <Action type="SetProperty" frame="LevelProgressBar/MaxLevelFillImage" Texture="@UI/SC2_UI_Coop_CommanderIcon_CircularProgress_Fill_Max_Capped"/>
                    <Action type="SetProperty" frame="CappedLevelLabel" Visible="false"/>
                    <Action type="SetProperty" frame="MaxLevelIcon" Visible="true"/>
                    <Action type="SetProperty" frame="LevelProgressBar" Visible="True"/>
                    <Action type="SetProperty" frame="RandomIcon" Visible="False"/>
                </State>
                
                <State name="MasteryMaxLevelState">
                    <When type="AnimationState" frame="$CoopStandardGlue_CommanderSelectionPanel" CommanderMaxLevel="MaxLevel"/> 
                    <When type="Property" frame="$Coop_MasteryLevelFrame" operator="GreaterThanOrEqual" MasteryLevel="91"/>
                    <Action type="SetProperty" frame="LevelLabel" style="@Coop_CommanderLevel_Max"/>
                    <Action type="SetProperty" frame="LevelLabel" Visible="true"/>
                    <Action type="SetProperty" frame="LevelProgressBar/BorderImage" texture="@UI/SC2_UI_Coop_CommanderIcon_CircularProgress_Border_Max"/>
                    <Action type="SetProperty" frame="LevelProgressBar/FillImageContainer/FillImage" visible="false"/>
                    <Action type="SetProperty" frame="LevelProgressBar/MaxLevelFillImage" visible="True"/>
                    <Action type="SetProperty" frame="LevelProgressBar/MaxLevelFillImage" Texture="@UI/SC2_UI_Coop_CommanderIcon_CircularProgress_Fill_Max"/>
                    <Action type="SetProperty" frame="CappedLevelLabel" Visible="false"/>
                    <Action type="SetProperty" frame="MaxLevelIcon" Visible="false"/>
                    <Action type="SetProperty" frame="LevelProgressBar" Visible="True"/>
                    <Action type="SetProperty" frame="RandomIcon" Visible="False"/>
                </State>
                
                <State name="MaxLevelState">
                    <When type="AnimationState" frame="$CoopStandardGlue_CommanderSelectionPanel" CommanderMaxLevel="MaxLevel"/> 
                    <Action type="SetProperty" frame="LevelLabel" style="@Coop_CommanderLevel_Max"/>
                    <Action type="SetProperty" frame="LevelLabel" Visible="true"/>
                    <Action type="SetProperty" frame="LevelProgressBar/BorderImage" texture="@UI/SC2_UI_Coop_CommanderIcon_CircularProgress_Border_Max"/>
                    <Action type="SetProperty" frame="LevelProgressBar/FillImageContainer/FillImage" visible="false"/>
                    <Action type="SetProperty" frame="LevelProgressBar/MaxLevelFillImage" visible="True"/>
                    <Action type="SetProperty" frame="LevelProgressBar/MaxLevelFillImage" Texture="@UI/SC2_UI_Coop_CommanderIcon_CircularProgress_Fill_Max"/>
                    <Action type="SetProperty" frame="CappedLevelLabel" Visible="false"/>
                    <Action type="SetProperty" frame="MaxLevelIcon" Visible="false"/>
                    <Action type="SetProperty" frame="LevelProgressBar" Visible="True"/>
                    <Action type="SetProperty" frame="RandomIcon" Visible="False"/>
                </State>

                <State name="NotMaxLevelCappedState">
                    <When type="AnimationState" frame="$CoopStandardGlue_CommanderSelectionPanel" CommanderMaxLevel="NotMaxLevel"/> 
                    <When type="Property" frame="$CoopStandardGlue_CommanderSelectionPanel/CommanderPurchaseButton" Visible="True"/> 
                    <When type="Property" frame="$CoopStandardGlue_CommanderSelectionPanel" operator="GreaterThanOrEqual" Level="5"/> 
                    <When type="StateGroup" CommanderCapState="Cappable"/>
                    <Action type="SetProperty" frame="LevelLabel" style="@Coop_CommanderLevel"/>
                    <Action type="SetProperty" frame="LevelLabel" Visible="false"/>
                    <Action type="SetProperty" frame="LevelProgressBar/BorderImage" texture="@UI/SC2_UI_Coop_CommanderIcon_CircularProgress_Border_Normal"/>
                    <Action type="SetProperty" frame="LevelProgressBar/FillImageContainer/FillImage" texture="@UI/SC2_UI_Coop_CommanderIcon_CircularProgress_Fill_Normal"/>
                    <Action type="SetProperty" frame="LevelProgressBar/MaxLevelFillImage" visible="True"/>
                    <Action type="SetProperty" frame="LevelProgressBar/MaxLevelFillImage" Texture="@UI/SC2_UI_Coop_CommanderIcon_CircularProgress_Fill_Normal_Capped"/>
                    <Action type="SetProperty" frame="CappedLevelLabel" Visible="true"/>
                    <Action type="SetProperty" frame="MaxLevelIcon" Visible="false"/>
                    <Action type="SetProperty" frame="LevelProgressBar" Visible="True"/>
                    <Action type="SetProperty" frame="RandomIcon" Visible="False"/>
                </State>

                <State name="NotMaxLevelState">
                    <When type="AnimationState" frame="$CoopStandardGlue_CommanderSelectionPanel" CommanderMaxLevel="NotMaxLevel"/> 
                    <Action type="SetProperty" frame="LevelLabel" style="@Coop_CommanderLevel"/>
                    <Action type="SetProperty" frame="LevelLabel" Visible="true"/>
                    <Action type="SetProperty" frame="LevelProgressBar/BorderImage" texture="@UI/SC2_UI_Coop_CommanderIcon_CircularProgress_Border_Normal"/>
                    <Action type="SetProperty" frame="LevelProgressBar/FillImageContainer/FillImage" texture="@UI/SC2_UI_Coop_CommanderIcon_CircularProgress_Fill_Normal"/>
                    <Action type="SetProperty" frame="LevelProgressBar/MaxLevelFillImage" visible="False"/>
                    <Action type="SetProperty" frame="CappedLevelLabel" Visible="false"/>
                    <Action type="SetProperty" frame="MaxLevelIcon" Visible="false"/>
                    <Action type="SetProperty" frame="LevelProgressBar" Visible="True"/>
                    <Action type="SetProperty" frame="RandomIcon" Visible="False"/>
                </State>
            </StateGroup>
        </Frame>
        
        
        <!-- Commander Custom Container -->

        <Frame type="Frame" name="CommanderCustomContainer" template="CM_CommanderSelectionDetailsContainer/UnitsDetailContainerTemplate">
            <Anchor side="Top" relative="$parent/DetailsBackground/AnchorFrame" pos="Min" offset="0"/>
            <Anchor side="Bottom" relative="$parent/DetailsBackground/AnchorFrame" pos="Max" offset="0"/>
            <Anchor side="Left" relative="$parent/DetailsBackground/DividerAnchor" pos="Max" offset="0"/>
            <Anchor side="Right" relative="$parent/DetailsBackground/AnchorFrame" pos="Max" offset="0"/>
        </Frame>

        
        <!-- Info Detail Container -->

        <Frame type="Frame" name="InfoContainer" template="CM_CommanderSelectionDetailsContainer/InfoDetailContainerTemplate">
            <Anchor relative="$parent/CommanderCustomContainer"/>
        </Frame>
        
        
        <!-- Upgrades Detail Container -->
        
        <Frame type="Frame" name="UpgradesContainer" template="CM_CommanderSelectionDetailsContainer/UpgradesDetailContainerTemplate">
            <Anchor relative="$parent/CommanderCustomContainer"/>
        </Frame>
            
        <!--<Frame type="Frame" name="AbilHitArea" template="StandardTemplates/HoverableFrameTemplate">
            <Anchor relative="$parent/DetailsBackground/CommanderDetailsBackground"/>
        </Frame>-->
        
        <StateGroup name="RandomState">
            <DefaultState val="NotRandom"/>
            
            <State name="Random">
                <When type="Property" frame="$parent" Commander="Random"/>
                <Action type="SetProperty" frame="CommanderCustomButton" Enabled="False"/>
                <Action type="SetProperty" frame="UpgradesButton" Enabled="False"/>
                <Action type="SetProperty" frame="TabControl" SelectedIndex="0"/>
            </State>
            
            <State name="NotRandom">
                <Action type="SetProperty" frame="CommanderCustomButton" Enabled="True"/>
                <Action type="SetProperty" frame="UpgradesButton" Enabled="True"/>
            </State>
        </StateGroup>
    </Frame>

</Desc>