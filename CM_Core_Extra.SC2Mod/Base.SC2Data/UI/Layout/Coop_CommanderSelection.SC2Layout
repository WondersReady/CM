    <!--
        分隔符
    -->
    <Frame type="Frame" name="CompositionInfoUpgrageIconTemplate">
        <Width val="45"/>
        <Height val="45"/>

        <Frame type="Image" name="bg">
            <Anchor relative="$parent"/>
            <Alpha val="255"/>
            <Color val="55,55,55"/>
            <Texture val="Assets/Textures/SC2_UI_Glues_AlliedCommanders_UpgradeBackground.dds"/>

            <Animation name="Lock">
                <Event event="Locked" action="Reset,Play" frame="$parent"/>
                <Event event="Unlocked" action="DirectionReverse,Play" frame="$parent"/>

                <Controller name="Fade_0" type="Fade" end="Pause" frame="$this">
                    <Key type="CurveReal" time="0" inout="Linear,Auto" value="255"/>
                    <Key type="CurveReal" time="0.010" inout="Linear,Auto" value="100"/>
                </Controller>
            </Animation>
        </Frame>

        <Frame type="Image" name="Icon">
            <Anchor relative="$parent"/>
            <Texture val="Assets/Textures/blank.dds"/>

            <Animation name="Unlock">
                <Event event="Locked" action="Reset,Play" frame="$parent"/>
                <Event event="Unlocked" action="DirectionReverse,Play" frame="$parent"/>

                <Controller name="Fade_0" type="Fade" end="Pause" frame="$this">
                    <Key type="CurveReal" time="0" inout="Linear,Auto" value="255"/>
                    <Key type="CurveReal" time="0.010" inout="Linear,Auto" value="0"/>
                </Controller>
            </Animation>
        </Frame>

        <Frame type="Image" name="locked">
            <Anchor side="Top" relative="$parent" pos="Mid" offset="0"/>
            <Anchor side="Bottom" relative="$parent" pos="Mid" offset="0"/>
            <Anchor side="Left" relative="$parent" pos="Mid" offset="0"/>
            <Anchor side="Right" relative="$parent" pos="Mid" offset="0"/>
            <Width val="25"/>
            <Height val="25"/>
            <Alpha val="0"/>
            <Texture val="Assets/Textures/SC2_UI_Glues_AlliedCommanders_Lock.dds"/>

            <Animation name="Lock">
                <Event event="Locked" action="Reset,Play" frame="$parent"/>
                <Event event="Unlocked" action="DirectionReverse,Play" frame="$parent"/>

                <Controller name="Fade_0" type="Fade" end="Pause" frame="$this">
                    <Key type="CurveReal" time="0" inout="Linear,Auto" value="0"/>
                    <Key type="CurveReal" time="0.010" inout="Linear,Auto" value="50"/>
                </Controller>
            </Animation>
        </Frame>
    </Frame>

    <Frame type="Frame" name="CommanderTalentTileListItemTemplate">
        <Width val="78"/>
        <Height val="78"/>
        <AcceptsMouseTooltip val="true"/>

        <Frame type="Image" name="BackgroundImage">
            <Anchor side="Top" relative="$parent" pos="Min" offset="3"/>
            <Anchor side="Bottom" relative="$parent" pos="Max" offset="-3"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="3"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="-3"/>
            <Texture val="Assets/Textures/white.dds"/>
            <Alpha val="179"/>
            <Color val="0,0,0"/>
        </Frame>

        <!-- <DesaturationColor val="2a3b50"/> -->
        <Frame type="Image" name="Icon">
            <Anchor side="Top" relative="$parent" pos="Min" offset="1"/>
            <Anchor side="Bottom" relative="$parent" pos="Max" offset="-1"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="1"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="-1"/>
            <ColorAdjustMode val="Colorize"/>
            <AdjustmentColor val="54,107,181"/>
            <LightAdjustColor val="129,156,203"/>
        </Frame>

        <Frame type="Image" name="LockedOverlay">
            <Anchor relative="$parent/Icon"/>
            <Visible val="false"/>
            <LayerCount val="2"/>
            <AlphaLayer val="true"/>
            <Texture val="{$parent/Icon/@Texture}"/>
            <Texture val="Assets/Textures/white.dds" Layer="1"/>
            <LayerAlpha val="153" Layer="1"/>
            <LayerColor val="0,0,0" Layer="1"/>
        </Frame>

        <Frame type="Image" name="LockedGlow">
            <Anchor relative="$parent/BackgroundImage"/>
            <Texture val="@UI/SC2_UI_Collection_VoicePack_CharacterFrame_GenericEffect_RadialGradient_256x256"/>
            <TextureCoords top="0.55" left="-0.5" bottom="1.5" right="0.45"/>
            <Alpha val="128"/>
            <Color val="54,94,131"/>
            <Visible val="{$parent/LockedOverlay/@Visible}"/>
        </Frame>

        <!-- BORDER OUTER -->
        <Frame type="Image" name="BorderOuter">
            <Anchor relative="$parent"/>
            <Texture val="@UI/Glues_Battlenet_PageAssets_GraphicListItem_Border_Outer"/>
            <TextureType val="NineSlice"/>
            <Color val="0,0,0"/>
        </Frame>

        <!-- BORDER INNER -->
        <Frame type="Image" name="BorderInner">
            <Anchor relative="$parent"/>
            <Texture val="@UI/Glues_Battlenet_PageAssets_GraphicListItem_Border_Inner"/>
            <TextureType val="NineSlice"/>
            <Color val="37,107,169"/>
        </Frame>

        <Frame type="Image" name="LevelBackground">
            <Anchor side="Bottom" relative="$parent" pos="Max" offset="-1"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="1"/>
            <Width val="25"/>
            <Height val="22"/>
            <Texture val="@UI/SC2_UI_Coop_AbilityFrame_LevelBG"/>
            <TextureType val="NineSlice"/>
        </Frame>

        <Frame type="Label" name="LevelLabel">
            <Anchor side="Bottom" relative="$parent" pos="Max" offset="-1"/>
            <Anchor side="Left" relative="$parent/LevelBackground" pos="Mid" offset="1"/>
            <Anchor side="Right" relative="$parent/LevelBackground" pos="Mid" offset="1"/>
            <Style val="Coop_CommanderTalentLevel_Earned"/>

            <Frame type="MathFrame" name="ConvertToInt">
                <Anchor relative="$parent"/>
                <A val="{$parent/@Text}"/>
                <B val="0"/>
                <Operator val="Add"/>
            </Frame>
        </Frame>

        <Frame type="Image" name="LockIcon">
            <Anchor side="Top" relative="$parent" pos="Min" offset="10"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="-10"/>
            <Texture val="@UI/SC2_UI_Coop_LockIcon_Upgrade"/>
            <DesaturateOnDisable val="false"/>
            <Visible val="{$parent/LockedOverlay/@Visible}"/>
        </Frame>

        <StateGroup name="LevelState">
            <DefaultState val="Below10"/>
            <State name="AtOrAbove10">
                <When type="Property" frame="LevelLabel/ConvertToInt" operator="GreaterThanOrEqual" ResultInt="10" Index="0"/>
                <Action type="SetProperty" frame="LevelBackground" Width="36"/>
            </State>
            <State name="Below10">
                <Action type="SetProperty" frame="LevelBackground" Width="25"/>
            </State>
        </StateGroup>

        <StateGroup name="EarnedState">
            <DefaultState val="Unearned"/>
            <State name="Earned">
                <When type="AnimationState" EarnedState="Earned"/>
                <Action type="SetProperty" frame="Icon" Desaturated="False"/>
                <Action type="SetProperty" frame="Icon" ColorAdjustMode="None"/>
                <Action type="SetProperty" frame="BorderInner" Color="37,107,169"/>
                <Action type="SetProperty" frame="LockedOverlay" Visible="False"/>
                <Action type="SetProperty" frame="LevelLabel" Style="Coop_CommanderTalentLevel_Earned"/>
                <Action type="SetProperty" frame="LevelBackground" Texture="@UI/SC2_UI_Coop_AbilityFrame_LevelBG"/>
            </State>
            <State name="Unearned">
                <Action type="SetProperty" frame="Icon" Desaturated="True"/>
                <Action type="SetProperty" frame="Icon" ColorAdjustMode="Colorize"/>
                <Action type="SetProperty" frame="BorderInner" Color="29,57,86"/>
                <Action type="SetProperty" frame="LockedOverlay" Visible="True"/>
                <Action type="SetProperty" frame="LevelLabel" Style="Coop_CommanderTalentLevel_Unearned"/>
                <Action type="SetProperty" frame="LevelBackground" Texture="@UI/SC2_UI_Coop_AbilityFrame_LevelBG_Disabled"/>
            </State>
        </StateGroup>
    </Frame>

    <!-- Note: There are animation events for the different talent types: TalentTypePassive, TalentTypeAbility, TalentTypeBundle, TalentTypeUnit -->
    <Frame type="Frame" name="MutatorShowFrameTemplate">
        <Width val="50"/>
        <Height val="51"/>
        <CollapseLayout val="false"/>
        <Visible val="false"/>
        <AcceptsMouse val="true"/>

        <Frame type="Image" name="MutatorIcon">
            <Anchor side="Top" relative="$parent" pos="Min" offset="6"/>
            <Anchor side="Bottom" relative="$parent" pos="Max" offset="-6"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="6"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="-6"/>
        </Frame>

        <Frame type="Image" name="BorderImage">
            <Anchor relative="$parent"/>
            <Texture val="Assets\Textures\ui_coop_ingame_mutatorFrame.dds"/>
            <TextureType val="Normal"/>
        </Frame>

        <StateGroup name="SourceState">
            <DefaultState val="Official"/>
            <State name="Official">
                <Action type="SetProperty" frame="BorderImage" Texture="assets/textures/ui_coop_ingame_mutatorframe.dds"/>
            </State>
            <State name="Plus">
                <When type="AnimationState" SourceState="Plus"/>
                <Action type="SetProperty" frame="BorderImage" Texture="assets/textures/ui_glues_profileavatar_grandmaster_small.dds"/>
            </State>
            <State name="ThirdParty">
                <When type="AnimationState" SourceState="ThirdParty"/>
                <Action type="SetProperty" frame="BorderImage" Texture="assets/textures/ui_glues_profileavatar_diamond_small.dds"/>
            </State>
        </StateGroup>
    </Frame>

    <Frame type="Button" name="MasteryButtonsTemplate">
        <Width val="230"/>
        <Height val="50"/>
        <NormalImage val="NormalImage"/>
        <HoverImage val="HoverImage"/>
        <ClickSound val="@UI_ActionButtonSelect"/>
        <Text val="@UI/Blank"/>

        <!--Label val="Label"/-->
