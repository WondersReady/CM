    <Frame type="Button" name="PrestigeButtonTemplate">
        <Width val="#PrestigeButtonSize"/>
        <Height val="#PrestigeButtonSize"/>
        <NormalImage val="NormalImage"/>
        <HoverImage val="HoverImage"/>

        <Frame type="Image" name="NormalImage">
