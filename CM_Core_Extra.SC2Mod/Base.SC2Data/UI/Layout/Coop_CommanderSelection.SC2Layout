            <Anchor side="Left" relative="$parent/Difficulty" pos="Max" offset="10"/>
            <Width val="36"/>
            <Height val="36"/>

            <Frame type="Image" name="Icon">
                <Anchor side="Top" relative="$parent" pos="Mid" offset="0"/>
